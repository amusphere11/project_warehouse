# Production Environment Variables
# Copy this file to .env.prod and update with your actual values

# Database Configuration
DB_USER=warehouse_user
DB_PASSWORD=CHANGE_THIS_PASSWORD_IN_PRODUCTION
DB_NAME=warehouse_db

# Redis Configuration
REDIS_PASSWORD=CHANGE_THIS_PASSWORD_IN_PRODUCTION

# JWT Configuration
JWT_SECRET=CHANGE_THIS_SECRET_IN_PRODUCTION_USE_STRONG_RANDOM_STRING
JWT_EXPIRES_IN=7d

# API Configuration (Optional)
# If you need to expose backend directly
BACKEND_PORT=3000

# Monitoring & Logging (Optional)
LOG_LEVEL=info
